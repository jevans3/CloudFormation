			"Value" : { "Fn::FindInMap":[ "myVPCSubnetConfig", { "Ref":"AWS::Region" }, "Public2" ] }
		},
		
		"PrivateSubnet1CIDR" : {
			"Description" : "Subnet CIDR of the Private      Tier Subnet AZ A",
			"Value" : { "Fn::FindInMap":[ "myVPCSubnetConfig", { "Ref":"AWS::Region" }, "Private1" ] }
		},
		
