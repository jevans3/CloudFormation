	"Resources":{
		"VPC":{
			"Type":"AWS::EC2::VPC",
			"Properties":{
				"EnableDnsSupport":"true",
				"EnableDnsHostnames":"true",
				"CidrBlock":{
					"Fn::FindInMap":[ "myVPCSubnetConfig", { "Ref":"AWS::Region" }, "VPC" ]
				},
				"Tags":[ { "Key":"Name", "Value":"myVPC" } ]
			}
		},
		
		"InternetGateway":{
			"Type":"AWS::EC2::InternetGateway",
			"Properties":{
				"Tags":[ { "Key":"Name", "Value":"myVPC IGW" } ]
			}
		},
		
		"GatewayToInternet":{
			"Type":"AWS::EC2::VPCGatewayAttachment",
			"Properties":{
				"VpcId":{ "Ref":"VPC" },
				"InternetGatewayId":{ "Ref":"InternetGateway" }
			}
		},
		
		"PublicRouteTable":{
			"Type":"AWS::EC2::RouteTable",
			"Properties":{
				"VpcId":{ "Ref":"VPC" },
				"Tags":[ { "Key" : "Name", "Value" : "Public RT" } ]
			}
		},
		
		"PublicRoute":{
			"Type":"AWS::EC2::Route",
			"DependsOn":"GatewayToInternet",
			"Properties":{
				"DestinationCidrBlock":"0.0.0.0/0",
				"GatewayId":{ "Ref":"InternetGateway" },
				"RouteTableId":{ "Ref":"PublicRouteTable" }
			}
		},
		
		"PrivateRouteTable":{
			"Type":"AWS::EC2::RouteTable",
			"Properties":{
				"VpcId":{ "Ref":"VPC" },
				"Tags":[ { "Key" : "Name", "Value" : "Private RT" } ]
			}
		},
		
		"DBRouteTable":{
			"Type":"AWS::EC2::RouteTable",
			"Properties":{
				"VpcId":{ "Ref":"VPC" },
				"Tags" : [ { "Key" : "Name", "Value" : "DB RT" } ]
			}
		},
		
		"IELBRouteTable":{
			"Type":"AWS::EC2::RouteTable",
			"Properties":{
				"VpcId":{ "Ref":"VPC" },
				"Tags" : [ { "Key" : "Name", "Value" : "Internal ELB RT" } ]
			}
		},
		
		"ExtELBSubnetA" : {
			"Type" : "AWS::EC2::Subnet",
			"Properties" : {
				"AvailabilityZone" : {
					"Fn::FindInMap" : [ "myVPCSubnetAZ", { "Ref" : "AWS::Region" }, "ExtELB1" ]
				},
				"CidrBlock" : {
					"Fn::FindInMap" : [ "myVPCSubnetConfig", { "Ref" : "AWS::Region" }, "ExtELB1" ]
				},
				"Tags" : [ { "Key" : "Name", "Value" : "ExtELB-A" } ],
				"VpcId" : { "Ref" : "VPC" }
			}
		},
		
		"ExtELBSunetASubnetRouteTableAssociation" : {
			"Type" : "AWS::EC2::SubnetRouteTableAssociation",
			"Properties" : {
				"RouteTableId" : { "Ref" : "PublicRouteTable" }, 
				"SubnetId" : { "Ref" : "ExtELBSubnetA" }
			}
		},
		
		"ExtELBSubnetB" : {
			"Type" : "AWS::EC2::Subnet",
			"Properties" : {
				"AvailabilityZone" : {
					"Fn::FindInMap" : [ "myVPCSubnetAZ", { "Ref" : "AWS::Region" }, "ExtELB2" ]
				},
				"CidrBlock" : {
